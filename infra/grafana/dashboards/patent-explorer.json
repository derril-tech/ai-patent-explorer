          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      },
      {
        "id": 14,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{job}}"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 48
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [],
    "annotations": {
      "list": []
    },
    "templating": {
      "list": []
    }

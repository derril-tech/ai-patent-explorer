{
  "dashboard": {
    "id": null,
    "title": "AI Patent Explorer - System Dashboard",
    "tags": ["patent-explorer", "ai", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{method}} {{endpoint}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Worker Jobs",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(worker_jobs_total[5m])",
            "legendFormat": "{{worker_type}} {{job_type}} - {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Patent Processing",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(patents_processed_total[5m])",
            "legendFormat": "{{processing_stage}} - {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Embeddings Generated",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(embeddings_generated_total[5m])",
            "legendFormat": "{{embedding_type}} - {{model}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Search Queries",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(search_queries_total[5m])",
            "legendFormat": "{{search_type}} - {{result_count}} results"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Alignments Created",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(alignments_created_total[5m])",
            "legendFormat": "{{alignment_type}} - {{confidence_level}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "Novelty Scores",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(novelty_scores_calculated_total[5m])",
            "legendFormat": "{{confidence_band}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Charts Generated",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(charts_generated_total[5m])",
            "legendFormat": "{{chart_type}} - {{format}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 10,
        "title": "Active Connections",
        "type": "stat",
        "targets": [
